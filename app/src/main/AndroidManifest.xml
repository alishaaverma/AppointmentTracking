<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.appointmenttracking">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ManagerLogin"></activity>
        <activity android:name=".EditClientDetails" />
        <activity android:name=".TrackList" />
        <activity android:name=".NewClientAdd" />
        <activity
            android:name=".AgentPage"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".ManagerPage"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".SecondScreen"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>